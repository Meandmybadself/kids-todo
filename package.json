{
  "name": "kids-todos",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "browserslist": "safari 9",
  "scripts": {
    "dev": "parcel index.html",
    "build": "rm -rf dist && parcel build index.html --no-cache --no-source-maps && npm run transpile",
    "transpile": "babel dist --out-dir dist --presets=@babel/preset-env"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@parcel/transformer-sass": "^2.8.3",
    "parcel": "^2.8.3",
    "sass": "^1.57.1",
    "typescript": "^4.9.3"
  }
}